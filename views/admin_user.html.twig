{% extends 'admin.html.twig' %}

{% block content %}
    <table>
        <tr>
            <th>Pesudo</th>
            <th>ID</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th class="size_delete">Supprimer</th>
        </tr>
        {% for user in users %}
        <tr>
            <td>{{ user['username'] }}</td>
            <td>{{ user['id'] }}</td>
            <td>{{ user['firstname'] }}</td>
            <td>{{ user['lastname'] }}</td>
            <td><img class="delete" src="web/img/delete.png" alt="delete user"></td>
        </tr>
        {% endfor %}
    </table>
{% endblock %}
